{
    "name": "destinio",
    "displayName": "destinio",
    "description": "Destinio vscode things",
    "icon": "icon.png",
    "publisher": "destinio",
    "repository": {
      "type": "git",
      "url": "https://github.com/destinio/destiniocodesnippets"
    },
    "version": "2.0.0",
    "engines": {
        "vscode": "^1.49.0"
    },
    "categories": [
        "Snippets",
        "Themes"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "javascript",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "javascriptreact",
                "path": "./snippets/snippets.code-snippets"
            },
            {
                "language": "css",
                "path": "./snippets/css.code-snippets"
            },
            {
                "language": "json",
                "path": "./snippets/json.code-snippets"
            },
            {
                "language": "jsonc",
                "path": "./snippets/json.code-snippets"
            }
        ],
        "themes": [
            {
                "label": "DestinioTheme",
                "uiTheme": "vs-dark",
                "path": "./themes/DestinioTheme.json"
            }
        ],
        "menus": {
          "explorer/context": [
            {
              "command": "workbench.files.action.collapseExplorerFolders"
            }
          ]
        }
    }
}
